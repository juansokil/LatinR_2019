@article{ ISI:000440513100020,
Author = {Lira, Mariana Sarmento and Rios do Nascimento, Luis Felipe},
Title = {{DISRESPECT OF THE SOCIO-AFFECTIVITY AND DEPROTECTION TO CHILDHOOD:
