@article{ ISI:000350769100003,
Author = {Goldenberg, Shira M. and Silverman, Jay G. and Engstrom, David and
   Bojorquez-Chapela, Ietza and Usita, Paula and Luisa Rolon, Maria and
   Strathdee, Steffanie A.},
Title = {{Exploring the Context of Trafficking and Adolescent Sex Industry
   Involvement in Tijuana, Mexico: Consequences for HIV Risk and Prevention}},
Journal = {{VIOLENCE AGAINST WOMEN}},
Year = {{2015}},
Volume = {{21}},
Number = {{4}},
Pages = {{478-499}},
Month = {{APR}},
Abstract = {{Coerced and adolescent sex industry involvement are linked to serious
   health and social consequences, including enhanced risk of HIV
   infection. Using ethnographic fieldwork, including interviews with 30
   female sex workers with a history of coerced or adolescent sex industry
   involvement, we describe contextual factors influencing vulnerability to
   coerced and adolescent sex industry entry and their impacts on HIV risk
   and prevention. Early gender-based violence and economic vulnerability
   perpetuated vulnerability to coercion and adolescent sex exchange, while
   HIV risk mitigation capacities improved with increased age, control over
   working conditions, and experience. Structural interventions addressing
   gender-based violence, economic factors, and HIV prevention among all
   females who exchange sex are needed.}},
