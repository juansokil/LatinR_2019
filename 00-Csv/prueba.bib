@article{ ISI:000440513100016,
Author = {D'Andrea, Luigi Silvino and Romagnoli, Roberta Carvalho},
Title = {{MOVING ON THE RAINBOW WITH BLACK AND GAY SHADES: ASEXUALITIES IN TRANSIT}},
Journal = {{REVISTA ARTEMIS}},
