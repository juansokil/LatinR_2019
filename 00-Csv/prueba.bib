@article{ ISI:000372173200015,
Author = {Connor, Avonne E. and Baumgartner, Kathy B. and Baumgartner, Richard N.
   and Pinkston, Christina M. and Boone, Stephanie D. and John, Esther M.
   and Torres-Mejia, Gabriela and Hines, Lisa M. and Giuliano, Anna R. and
