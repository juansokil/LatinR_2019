@article{ ISI:000372198500019,
Author = {Perez, Moira},
Title = {{(?Y ES ESE REALMENTE UN LUGAR TAN MALO PARA ESTAR?)}},
Journal = {{REVISTA ARTEMIS}},
Year = {{2015}},
Volume = {{20}},
Pages = {{187-192}},
Month = {{AUG-DEC}},
Abstract = {{The text is a part of My gender workbook (1998), in which Kate Bornstein
   invites us to a playful journey of creative exploration of gender -our
   own, and others'. In this chapter, Bornstein offers her theoretical
   development on the notion of gender, examining some common conceptions
   and considering a number of alternatives. Finally, after looking into
   what would constitute a ``perfect gender{''} according to our society's
   current cultural order, she arrives at the idea of a
   ``Gender/Identity/Power pyramid{''}, in which gender is interwoven with
   various identity categories which, according to Bornstein, are hidden
   behind binary notions of gender, thus rendering invisible their power to
   place different subjects in different positions within social
   hierarchies. This translation into Spanish offers core notions from a
   fundamental author within Queer theory, whose work has not been, to this
   day, translated into Spanish.}},
