Language = {{English}},
Affiliation = {{Rodero, E (Reprint Author), Pompeu Fabra Univ UPF, Dept Commun, Roc Boronat 138, Barcelona 08018, Spain.
   Rodero, Emma; Larrea, Olatz, Pompeu Fabra Univ UPF, Dept Commun, Barcelona 08018, Spain.
   Vazquez, Marina, Fac Commun, Colonia Las Viboras 28040, Colima, Mexico.}},
DOI = {{10.1007/s11199-012-0247-y}},
