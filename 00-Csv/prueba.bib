@article{ ISI:000313810200002,
Author = {Azambuja, Mariana and Nogueira, Conceicao and Neves, Sofia and de
   Oliveira, Joao Manuel},
Title = {{Gender Violence in Portugal: Discourses, Knowledge and Practices}},
Journal = {{INDIAN JOURNAL OF GENDER STUDIES}},
