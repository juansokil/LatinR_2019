@article{ ISI:000285876300012,
Author = {de Mendonca, Julia Scarano and Cossette, Louise and Strayer, Francis F.
   and Gravel, France},
Title = {{Mother-Child and Father-Child Interactional Synchrony in Dyadic and
   Triadic Interactions}},
Journal = {{SEX ROLES}},
Year = {{2011}},
Volume = {{64}},
Number = {{1-2}},
Pages = {{132-142}},
Month = {{JAN}},
Abstract = {{Patterns of interactional synchrony were compared in mother-child and
   father-child dyads during dyadic and triadic interactions. Forty-two
   dual-earner families from the French province of Qu,bec, Canada,
   participated in the study with their 32 months-old child. Parent-child
   interactions were coded using a taxonomy including the social partners'
   physical proximity, visual and body orientation, and dyadic involvement.
   Analyses show similar patterns of interactional synchrony in
   mother-child and father-child dyads in the dyadic context while
   father-child dyads presented less interactional synchrony than
   mother-child dyads when interacting in triad. Discussion focuses on the
   impact of the context and on the factors that could explain the changes
   in father-child patterns of interaction from one context to another.}},
