@article{ ISI:000393409600004,
Author = {Consul, Marnei and Amorim, Filipi Vieira and Neves, Julia Guimaraes},
Title = {{FROM INERTIA TO THE POSSIBLE: THE CHALLENGES OF TEACHING WHILE TAKING
   INTO ACCOUNT GENDER AND SEXUALITY ISSUES}},
Journal = {{REVISTA ARTEMIS}},
Year = {{2016}},
Volume = {{21}},
Pages = {{37-49}},
Month = {{JAN-JUL}},
Abstract = {{This is a theoretical approach on the issue of homosexuality. The goal
   is to build a reflection on the role of the school in the dealings with
   issues related to gender and sexuality studies. So, we intend to
   approach the teachers to the topic and thus provide further reflection
   regarding the homosexual students. We will discuss the concept of
   homosexualism, addressing issues such as the outdated classification of
   the concept of ``the homosexual{''} as a term that, since its
   ety-mology, refers, according to medical accounts, to diseases; We will
   also discuss scientific determinism and genetic influence on human
   behavior; the educational challenge and the contemporary task of dealing
   with diversity in the schools. We conclude that the school is one of the
   important places to teach how to live with diversity, in diversity and
   for diversity, although this is not a reality yet.}},
