@article{ ISI:000238177500006,
Author = {Barrancos, Dora},
Title = {{Problematic modernity - Gender, sexuality, and reproduction in
   twentieth-century Argentina}},
Journal = {{JOURNAL OF WOMENS HISTORY}},
Year = {{2006}},
Volume = {{18}},
Number = {{2}},
Pages = {{123-150}},
Month = {{SUM}},
Abstract = {{This article examines the limits of twentieth-century Argentine
   liberalism ill light Of two important rights for women: divorce and free
   access to contraceptive methods. Taking into account the liberal impulse
   of modernity at the beginning of the century and a phase of
   ``neomodernity{''} upheld by neoliberal forces at the end of the
   century, the article argues that, while modernizing phenomena often
   foretell a greater rationality, secularity, and consolidation of private
   rights, such was not the case in Argentina because of the extreme
   influence of the Catholic Church. The article concludes with a
   discussion of how liberalism was inflected by its encounters with the
   Catholic Church, whose influence managed to delay passage of a divorce
   law until 1987,forestall free access to contraceptive methods, and block
   the legalization of abortion.}},
