@article{ ISI:000440513100010,
Author = {Muller, Janaina W. and Schmidt, Sarai P.},
Title = {{THE GOOD GIRL MANUAL: REFLECTIONS ON FEMALE BEHAVIOR IN THE GOLDEN AND
   CONTEMPORARY YEARS}},
Journal = {{REVISTA ARTEMIS}},
Year = {{2018}},
Volume = {{25}},
Number = {{1}},
Pages = {{113-129}},
Month = {{JAN-JUN}},
Abstract = {{The article proposes a reflection about the rules of conduct imposed on
   the feminine gender, focusing on the youth suggested by the word
   ``girl{''}. In methodological terms, the study develops a
   historical-cultural discussion about the standardization and gender
   roles from two sources: the first one referring to the Golden Years in
   the section ``From Woman to Woman{''}, whose excerpts were taken from
   issues of the year 1953 of the magazine ``O Cruzeiro{''}; the second, in
   the association with the contemporaneity, consists of the video ``Why is
   woman vulgar?{''}, authored by Ellora Haonne and published in the year
   2017 on YouTube. The analyzes are based on the demarcations that define
   the feminine according to social expectations, exemplified by the ``good
   girl{''} and the ``vulgar woman{''}.}},
