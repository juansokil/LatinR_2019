@article{ ISI:000277575700002,
Author = {Smid, Marcela and Campero, Lourdes and Cragin, Leslie and Gonzalez
   Hernandez, Dolores and Walker, Dilys},
Title = {{Bringing Two Worlds Together: Exploring the Integration of Traditional
   Midwives as Doulas in Mexican Public Hospitals}},
Journal = {{HEALTH CARE FOR WOMEN INTERNATIONAL}},
Year = {{2010}},
Volume = {{31}},
Number = {{6}},
Pages = {{475-498}},
Abstract = {{Integrating traditional midwives (TMs) as labor support in cline-based
   care may be an ideal solution to improving maternity services in the
   Mexico and worldwide. We conducted interviews and focus groups with 65
   TMs and 24 interviews with public health system personnel to assess the
   acceptability and challenges of this proposal. Both TMs and personnel
   perceive this new role as having professional benefits. Challenges
   include TMs' and clinic personnel's previous negative experience with
   one another and unfamilitarity with the doula role. Interactive
   trainings introducing the doula role and improving professional
   relations between TMs and personnel are necessary to assure success of
   this proposal.}},
