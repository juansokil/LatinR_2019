@article{ ISI:000372416200005,
Author = {DeSisto, Carla L. and McDonald, Jill A. and Rochat, Roger and
   Diaz-Apodaca, Beatriz A. and Declercq, Eugene},
Title = {{Decision Making About Method of Delivery on the U.S.-Mexico Border}},
Journal = {{HEALTH CARE FOR WOMEN INTERNATIONAL}},
Year = {{2016}},
Volume = {{37}},
Number = {{4}},
Pages = {{426-443}},
Month = {{APR 2}},
Abstract = {{We explored how low-risk, nulliparous pregnant women and their doctors
   in two contiguous U.S.-Mexico border communities communicate about
   methods of delivery and how they perceive that the delivery method
   decision is made. We recruited 18 women through obstetricians in El
   Paso, Texas (n = 10), and prenatal care providers in Ciudad Juarez,
   Mexico (n = 8). We observed prenatal care visits, interviewed women
   prenatally and postpartum, and interviewed the El Paso obstetricians.
   Qualitative analysis demonstrated that birthing decisions are complex
   and involve multiple influences, including women's level of knowledge
   about birth, doctor-patient communication, and women's participation in
   decision making.}},
