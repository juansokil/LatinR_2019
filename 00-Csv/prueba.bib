@article{ ISI:000411596700002,
Author = {Pecanha, Natalia Batista},
Title = {{``PRECISA-SE DE UM HOMEM PARA TODO O SERVICE DE CASA{''}: MALE
   PARTICIPATION IN DOMESTIC SERVICE IN RIO DE JANEIRO (1880-1920)}},
Journal = {{REVISTA ARTEMIS}},
Year = {{2017}},
Volume = {{23}},
Number = {{1}},
Pages = {{4-12}},
Month = {{JAN-JUN}},
Abstract = {{To talk about domestic service is to refer usually exclusively to the
   service performed by mostly black women who performed all the services
   of a household. However, academic productions have unveiled the
   complexity of domestic service around the world, showing that the image
