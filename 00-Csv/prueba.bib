@article{ ISI:000372198500015,
Author = {Oliveira Denega, Alessa Montalvao and Duarte Alves, Ivia Iracema},
Title = {{GENDER DISCOURSE IN AUTOMOBILE ADVERTISING: IT'S ALL FEMINISM'S FAULT}},
Journal = {{REVISTA ARTEMIS}},
Year = {{2015}},
Volume = {{20}},
Pages = {{153-158}},
Month = {{AUG-DEC}},
Abstract = {{The aim of this research is to give visibility to representations of
   gender relations in audiovisual car advertisement - a a product attached
   to symbols of power and recognition in the consumist society, which has
   been destined almost exclusively to male audiences. For the discussion,
   I propose the analysis of Ford Fusion advertising - broadcasted by
   Brazilian television in 2009, also available on the internet. This
   advertising piece does not overcome the asymmetrical power relations and
   can even reinforce the myth ``all the fault of feminism{''}, promoting
   the implicit idea that women desire to surpass men, presenting an often
   invisible common sense question: would this be a ``feminist wish{''}? I
   hope with this research to discuss which symbolic ways of operating
   ideology are being promoted and to contribute to the understanding of
   media messages involving gender and power relations, while estimulating
   critical percebptions involved in the learning to read discourses and
   the implicit meanings.}},
