@article{ ISI:000215949200014,
Author = {Spaggiari Souza, Celso Antonio and Santos Freitas, Rita de Cassia},
Title = {{LATENT INFERIORITY TO COMPULSORY LEADING ROLE: THE BIASED LOOK AUGUSTE
   DE SAINT-HILAIRE ON WOMEN OF MINAS GERAIS IN THE EARLY DECADES OF THE
   NINETEENTH CENTURY}},
Journal = {{REVISTA ARTEMIS}},
Year = {{2015}},
Volume = {{19}},
Pages = {{90-100}},
Month = {{JAN-JUL}},
Abstract = {{This study aims to analyze the narratives of the French naturalist
   Auguste de Saint-Hilaire on women of Minas Gerais in the early
   nineteenth century. That produced the reports on the various trips he
   made to the interior of Brazil, this reviewer describes populations
   their customs and makes repeated references to women who finds in his
   ways. This Travel Writing, even if expose the marks of the European
   Enlightenment, and is loaded from the outside view, constitute a
   legitimate source of information about relationships between men and
   women in the period into our land. All explicit references to women in
   four of his books published in France and later in Brazil are analyzed.
   The argument that leads our analysis is that the relationships between
   men and women were far more complex than the stereotypes of the feminine
   condition usually express both today as yesterday.}},
