@article{ ISI:000229871800004,
Author = {Carneiro, S},
Title = {{Paint it black}},
Journal = {{NOUVELLES QUESTIONS FEMINISTES}},
Year = {{2005}},
Volume = {{24}},
Number = {{2}},
Pages = {{27+}},
Abstract = {{The Eurocentric vision of feminism prevents the question of race from
   being taken into account when analyzing women's situations, and leads to
   the universalization of the values of one particular culture and their
   application to all women. ``Blackening feminism{''} means taking racism
   as a system of oppression with an enormous impact on gender relations in
   all societies, and integrating the consequences of this position within
   feminist praxis and theory. It means recognizing the different
   historical experiences of women and how, in particular, sexism combines
   with racism in the experiences of racialized groups such as
   Afro-descendant women.}},
