@article{ ISI:000380315800004,
Author = {Debert, Guita Grin and Gregori, Maria Filomena},
Title = {{Conceptualising violence and gender in the Brazilian context: New issues
   and old dilemmas}},
Journal = {{FEMINIST THEORY}},
Year = {{2016}},
Volume = {{17}},
Number = {{2, SI}},
Pages = {{175-190}},
Month = {{AUG}},
Abstract = {{This article examines conceptualisations of violence against women
   developed in Brazilian feminism, and in legal and institutional measures
   against violence, from the 1980s to the present. Based on ethnographic
   studies carried out at the Women's Police Stations and Special Criminal
   Courts, and the controversies surrounding the 2006 Brazilian Law on
   domestic and familial violence, the authors map the meanings of
   expressions such as violence against women', marital violence', domestic
   violence', family violence' and gender violence'. The article reveals
   that the discourse that transforms violence into crime, in the Brazilian
   context, leads to semantic and institutional developments that replace
   an interest in politicising justice for the defence of women with the
   judicialisation of family relations.}},
