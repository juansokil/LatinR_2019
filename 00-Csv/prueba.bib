@article{ ISI:000430227700005,
Author = {Ortiz Rodriguez, Jeyle and Picazzo Palencia, Esteban and Alvarado
   Lagunas, Elias},
Title = {{The Effect of Different Forms of Violence on Women's Attitudes Toward
   Gender Equality and Decision-Making Capacity}},
Journal = {{AFFILIA-JOURNAL OF WOMEN AND SOCIAL WORK}},
Year = {{2018}},
Volume = {{33}},
Number = {{2}},
Pages = {{193-207}},
Month = {{MAY}},
Abstract = {{The purpose of this article is to analyze the effect of different forms
   of violence on Mexican women's attitudes and decision-making capacity.
   Specifically, attitudes toward gender equality are included as a
   mediator between violence and women's decision-making capacity. Results
   reveal that attitudes are a partial mediator between physical, sexual,
   and psychological violence and women's decision-making capacity. While
   physical, economic, and sexual violence negatively affect women's
   decision-making capacity, economic violence against women increases
   their involvement in decision-making.}},
