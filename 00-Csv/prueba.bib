@article{ ISI:000334390900002,
Author = {Luna, Florencia},
Title = {{CHALLENGES FOR ASSISTED REPRODUCTION AND SECONDARY INFERTILITY IN LATIN
   AMERICA}},
Journal = {{INTERNATIONAL JOURNAL OF FEMINIST APPROACHES TO BIOETHICS}},
Year = {{2014}},
Volume = {{7}},
Number = {{1}},
Pages = {{3-27}},
Month = {{SPR}},
Abstract = {{This essay explores a new way to think about Assisted Reproductive
   Technologies (ARTs) in the Latin American context. The infertility
   caused by inadequately treated sexually transmitted diseases and by
   unsafe or illegal abortions is preventable, reflects the neglect of
   women's sexual health, and disproportionally affects the region's
   poorest women. I suggest a new logic that revisits ARTs, to do justice
   to the relevance of this type of infertility. I propose utilizing these
   technologies so as to transform the care of women and open a new
   discourse. I also suggest that we be more strategic and seek new
   alliances.}},
