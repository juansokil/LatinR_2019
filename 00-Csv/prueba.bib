@article{ ISI:000423926500002,
Author = {de Almeida, Marcia},
Title = {{A PECULIAR REPRESENTATION OF THE FEMALE BODY IN THE SOMALI DIASPORA}},
Journal = {{REVISTA ARTEMIS}},
Year = {{2017}},
Volume = {{24}},
Number = {{1}},
Pages = {{3-11}},
Month = {{JUL-DEC}},
Abstract = {{The attribution of stereotyped identities has been a limiting element
   for women and cultures taken as ``others{''}, pushed into subaltern
   roles according to which individual identities, conflicts, questionings
   and changes were not considered. However, in respect to women writers
   coming from Italian ex-colonies in Africa such subjectivities prove to
   be very alive and changing, expressing themselves and producing some new
   literature, which crosses territorial and symbolic boundaries. Thus,
   this article, based on postcolonial and gender theories, proposes the
   critical reading of Madre piccola, a novel by the Italian Somali writer
   Cristina Ubax Ali Farah, focusing on peculiar representations of the
   female body, a body which denounces the colonial past, revealing the
   continuity of opressive relations until the present.}},
